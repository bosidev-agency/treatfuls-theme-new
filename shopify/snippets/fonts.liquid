{% style %}
  @font-face {
    font-family: "Founders Grotesk";
    src: url('{{ 'FoundersGrotesk-Regular.eot' | asset_url }}') format("embedded-opentype"),
        url('{{ 'FoundersGrotesk-Regular.woff' | asset_url }}') format("woff"),
        url('{{ 'FoundersGrotesk-Regular.ttf' | asset_url }}') format("truetype");
    font-weight: 400;
    font-style: normal;
  }
  
  @font-face {
    font-family: "Founders Grotesk";
    src: 
      url('{{ 'FoundersGroteskMono-Medium.eot' | asset_url }}') format("embedded-opentype"),
      url('{{ 'FoundersGroteskMono-Medium.woff' | asset_url }}') format("woff"),
      url('{{ 'FoundersGroteskMono-Medium.ttf' | asset_url }}') format("truetype");
    font-weight: 500;
    font-style: normal;
  }
  
  @font-face {
    font-family: "Founders Grotesk";
    src: 
      url('{{ 'FoundersGrotesk-Bold.eot' | asset_url }}') format("embedded-opentype"),
      url('{{ 'FoundersGrotesk-Bold.woff' | asset_url }}') format("woff"),
      url('{{ 'FoundersGrotesk-Bold.ttf' | asset_url }}') format("truetype");
    font-weight: bold;
    font-style: normal;
  }
{% endstyle %}